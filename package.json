{
  "name": "davi-monorepo",
  "version": "1.0.0",
  "description": "Monorepo for DAVI",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "clean": "turbo run clean",
    "compile": "turbo run compile",
    "dev-script": "turbo run dev --scope=\"dev-scripts\"",
    "davi-dev": "turbo run dev --filter=davi-frontend",
    "dev": "turbo run dev"
  },
  "pnpm": {
    "overrides": {
      "electron": "^17.1.2",
      "leveldown": "^6.1.1"
    }
  },
  "devDependencies": {
    "turbo": "^1.6.3"
  }
}
