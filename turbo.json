{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "clean": {},
    "compile": {
      "dependsOn": ["^clean"]
    },
    "setup": {
      "dependsOn": ["^compile"]
    },
    "dev": {
      "dependsOn": ["^setup"]
    },
    "build": {
      "dependsOn": ["^setup"]
    },
    "lint": {
      "dependsOn": ["^setup"]
    },
    "test": {
      "dependsOn": ["^setup"]
    }
  }
}
