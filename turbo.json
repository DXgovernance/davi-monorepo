{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "clean": {
      "dependsOn": ["^clean"]
    },
    "compile": {
      "dependsOn": ["^compile", "clean"]
    },
    "setup": {
      "dependsOn": ["^setup", "compile"]
    },
    "dxdao-contracts#compile": {
      "dependsOn": ["clean"],
      "outputs": ["artifacts/**"]
    },
    "dxdao-contracts#setup": {
      "dependsOn": ["dxdao-contracts#compile"],
      "outputs": ["types/**"]
    },
    "dev-scripts#compile": {
      "dependsOn": ["^dxdao-contracts#compile", "^dxdao-contracts#setup"],
      "cache": false
    },
    "dev-scripts#dev": {
      "dependsOn": ["dev-scripts#compile"],
      "outputs": ["build/**", "artifacts/**"]
    },
    "dao-subgraph#dev": {
      "cache": false
    },
    "guilds-subgraph#dev": {
      "cache": false
    },
    "dev": {},
    "build": {
      "dependsOn": ["^setup"]
    },
    "lint": {
      "dependsOn": ["^setup"]
    },
    "test": {
      "dependsOn": ["^setup", "^dxdao-contracts#compile"]
    },
    "pre-commit": {},
    "format-check": {}
  }
}
