{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "clean": {
      "dependsOn": ["^clean"]
    },
    "compile": {
      "dependsOn": ["^compile", "clean"]
    },
    "setup": {
      "dependsOn": ["^setup", "compile"]
    },
    "dxdao-contracts#compile": {
      "dependsOn": ["clean"],
      "outputs": ["artifacts/**"]
    },
    "dev-scripts#compile": {
      "dependsOn": ["^dxdao-contracts#compile"]
    },
    "dev-scripts#dev": {
      "dependsOn": ["dev-scripts#compile"]
    },
    "dxdao-subgraph#dev": {
      "dependsOn": ["^dev-scripts#compile"]
    },
    "dev": {
      "dependsOn": ["setup", "^dxdao-contracts#compile", "^dev-scripts#compile"]
    },
    "build": {
      "dependsOn": ["^setup"]
    },
    "lint": {
      "dependsOn": ["^setup"]
    },
    "test": {
      "dependsOn": ["^setup", "^dxdao-contracts#compile"]
    },
    "pre-commit": {},
    "format-check": {}
  }
}
